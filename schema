 
 CREATE DATABASE veggies;
 USE DATABASE;
 
 CREATE TABLE details(
   name          VARCHAR(32) NOT NULL PRIMARY KEY,
   dtm           INTEGER  NOT NULL,
   harvestwindow INTEGER  NOT NULL,
  sowingDates   VARCHAR(22)
);
 
